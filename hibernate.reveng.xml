<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="users" catalog="innoshare" class="UsersForm">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="ideas" catalog="innoshare" class="IdeasForm">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="ideas_discussion" catalog="innoshare">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="ideas_updates" catalog="innoshare">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="projects" catalog="innoshare" class="ProjectsForm">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
	<table name="ideas_images_map" catalog="innoshare">
		<primary-key>
			<generator class="identity"></generator>
		</primary-key>
	</table>
<table name="projects_images_map" catalog="innoshare">
	<primary-key>
		<generator class="identity"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>